ages = [19, 22, 19, 24, 20, 25, 26, 24, 25, 24]

ages.sort();
print('max age {}'.format(ages[-1]))
print('min age {}'.format(ages[0]))
print(ages)

if(len(ages)%2 == 0):
  print('median age {}'.format(int(((ages[len(ages)//2 - 1]) + (ages[len(ages)//2]))/2)))
else:
  print('median age {}'.format(ages[len(ages)//2 - 1]))

print("Average {}".format(sum(ages)/len(ages)))

avg = sum(ages)/len(ages)

print("Range {} - {}".format(min(ages),max(ages)))

print(abs(min(ages)-avg), abs(max(ages)-avg))

